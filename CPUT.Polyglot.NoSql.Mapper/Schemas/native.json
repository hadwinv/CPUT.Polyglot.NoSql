{
	"schemas": {
		"graph": [
			{
				"neo4j": [
					{
						"faculty": {
							"facultyid": "int:type",
							"description": "string:type"
						}
					},
					{
						"course": {
							"courseid": "int:type",
							"description": "string:type",
							"relations": [
								{
									"type": "node",
									"label": "OFFERED_IN",
									"reference": "faculty"
								},
								{
									"type": "node",
									"label": "CONTAINS",
									"reference": "subject"
								}
							]
						}
					},
					{
						"subject": {
							"subjectid": "int:type",
							"description": "string:type",
							"relations": [
								{
									"type": "node",
									"label": "REGISTERED",
									"reference": "progress"
								}
							]
						}
					},
					{
						"pupil": {
							"title": "string:type",
							"initial": "string:type",
							"name": "string:type",
							"surname": "string:type",
							"idnumber": "string:type",
							"dateofbirth": "datetime:type",
							"address": "address:type",
							"contact": "contact:type",
							"registration": "registration:type",
							"transcript": "transcript:type",
							"relations": [
								{
									"type": "node",
									"label": "ENROLLED_IN",
									"reference": "course"
								},
								{
									"type": "node",
									"label": "TRANSCRIPT",
									"reference": "progress"
								},
								{
									"type": "node",
									"label": "CITIZEN_OF",
									"reference": "country"
								},
								{
									"type": "node",
									"label": "LIVES_IN",
									"reference": "city"
								}
							]
						}
					},
					{
						"country": {
							"description": "string:type"
						}
					},
					{
						"city": {
							"subjectid": "int:type",
							"description": "string:type",
							"relations": [
								{
									"type": "node",
									"label": "IS_LOCATED_IN",
									"reference": "country"
								}
							]
						}
					},
					{
						"progress": {
							"pupil": "string:type",
							"course": "string:type",
							"subject": "string:type",
							"marks": "double:type",
							"percentage": "double:type",
							"grade": "string:type"
						}
					}
				]
			}
			
		],
		"document": [
			{
				"mongodb": [
					{
						"faculties": {
							"id": "int:type",
							"name": "string:type",
							"department_head": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "contact"
								}
							]
						}
					},
					{
						"courses": {
							"id": "int:type",
							"name": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-Many",
									"reference": "subjects"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "faculties"
								}
							]
						}
					},
					{
						"subjects": {
							"id": "int:type",
							"name": "string:type",
							"duration": "int:type",
							"cost": "decimal:type"
						}
					},
					{
						"people": {
							"title": "string:type",
							"initial": "string:type",
							"name": "string:type",
							"surname": "string:type",
							"id_number": "string:type",
							"date_of_birth": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "contact"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "enrollment"
								}
							]
						}
					},
					{
						"enrollment": {
							"id": "int:type",
							"registration_date": "datetime:type",
							"status": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "courses"
								}
							]
						}
					},
					{
						"address": {
							"id": "int:type",
							"street": "string:type",
							"city": "string:type",
							"country": "string:type"
						}
					},
					{
						"contact": {
							"id": "int:type",
							"email": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "address"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "phone"
								}
							]
						}
					},
					{
						"phone": {
							"mobile": "string:type",
							"home": "string:type"
						}
					}
				]
			}
		],
		"columnar": [
			{
				"cassandra": [
					{
						"scholar": {
							"idno": "string:type",
							"relations": [
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "person"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "admission"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "performance"
								},
								{
									"type": "table",
									"label": "One-To-One",
									"reference": "location"
								}
							]
						}
					},
					{
						"person": {
							"id": "int:type",
							"firstname": "string:type",
							"lastname": "string:type",
							"dob": "string:type",
							"email": "string:type",
							"cellnumber": "string:type",
							"homenumber": "string:type"
						}
					},
					{
						"admission": {
							"course": "string:type",
							"subject": "string:type"
						}
					},
					{
						"performance": {
							"subject": "string:type",
							"marks": "decimal:type",
							"grade": "string:type"
						}
					},
					{
						"location": {
							"streetno": "int:type",
							"streetname": "string:type",
							"postalcode": "string:type",
							"suburb": "string:type",
							"city": "string:type"
						}
					}
				]
			}
		],
		"keyvalue": [
			{
				"redis": [
					{
						"user": {
							"identity_number": "string:type",
							"first_name": "string:type",
							"last_name": "string:type",
							"preferred_name": "string:type",
							"user_name": "string:type",
							"pwd": "string:type",
							"ip_address": "string:type",
							"device": "string:type",
							"session_id": "string:type",
							"login_date": "datetime:type",
							"logout_date": "datetime:type"
						}
					}
				]
			}	
		]
	}
}